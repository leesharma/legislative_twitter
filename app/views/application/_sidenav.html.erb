<div class="large-3 pull-9 columns">

  <h4>Search</h4>

  <%= form_tag search_path, :method => 'get' do %>
      <div class="row collapse">
        <div class="small-10 columns">
          <%= text_field_tag :search, params[:search], placeholder: 'Search Terms' %>
        </div>
        <div class="small-2 columns">
          <%= submit_tag "Go", :name => nil, class: 'button postfix' %>
        </div>
      </div>
  <% end %>

  <h4>Links</h4>
  <ul class="side-nav">
    <li><%= link_to 'Legislation', legislations_path %></li>
    <li><%= link_to 'Statuses', statuses_path %></li>
    <li><%= link_to 'Organizations', organizations_path %></li>
    <li><%= link_to 'Meetings', meetings_path %></li>
    <li><%= link_to 'Dockets', dockets_path %></li>
    <!--<li><a href="#">Section 6</a></li>-->
  </ul>


</div>